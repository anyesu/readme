{
	"name": "@anyesu/readme",
	"version": "1.3.0",
	"description": "Automatically generate a beautiful best-practice README file based on the contents of your repository",
	"license": "MIT",
	"module": "index.esm.js",
	"main": "index.cjs.js",
	"types": "index.d.ts",
	"author": "Appnest",
	"scripts": {
		"lib": "rollup -c && rollup -c=rollup-cli.config.js",
		"build": "node prebuild.js && npm run lib",
		"readme": "npm run lib && npm run generate:readme",
		"generate:readme": "node dist/cli.cjs.js generate",
		"publish:npm": "cd dist && npm publish",
		"push:readme": "npm run readme && git add . && git commit -m 'New readme' && git push",
		"release": "npx semantic-release",
		"ncu": "ncu -u -a && npm update && npm install"
	},
	"bugs": {
		"url": "https://github.com/anyesu/readme/issues"
	},
	"homepage": "https://github.com/anyesu/readme#readme",
	"repository": {
		"type": "git",
		"url": "git+https://github.com/anyesu/readme.git"
	},
	"keywords": [
		"opensource",
		"project",
		"readme",
		"template",
		"boilerplate",
		"nodejs",
		"maintaining",
		"generator"
	],
	"contributors": [
		{
			"name": "Andreas Mehlsen",
			"url": "https://twitter.com/andreasmehlsen",
			"img": "https://avatars1.githubusercontent.com/u/6267397?s=460&v=4",
			"info": [
				"ðŸ”¥"
			]
		}
	],
	"bin": {
		"readme": "cli.cjs.js"
	},
	"dependencies": {
		"@types/glob": "^7.1.1",
		"check-links": "^1.1.8",
		"colors": "^1.4.0",
		"commander": "^5.0.0",
		"fs-extra": "^9.0.0",
		"glob": "^7.1.6",
		"path": "^0.12.7",
		"web-component-analyzer": "1.0.3"
	},
	"devDependencies": {
		"@bobvanderlinden/semantic-release-pull-request-analyzer": "1.0.1",
		"@semantic-release/exec": "6.0.3",
		"@semantic-release/git": "10.0.1",
		"@types/fs-extra": "^8.1.0",
		"@types/node": "14.14.31",
		"@wessberg/rollup-plugin-ts": "^1.2.21",
		"rimraf": "^3.0.2",
		"rollup": "2.1.0",
		"rollup-plugin-commonjs": "^10.1.0",
		"rollup-plugin-json": "^4.0.0",
		"rollup-plugin-node-resolve": "^5.2.0",
		"semantic-release": "23.1.1",
		"typescript": "3.8.3"
	},
	"publishConfig": {
		"access": "public",
		"provenance": true
	}
}
